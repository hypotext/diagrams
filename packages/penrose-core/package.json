{
  "name": "penrose-core",
  "version": "1.0.0",
  "main": "build/dist/index.js",
  "module": "build/dist/index.esm.js",
  "types": "build/dist/types.d.ts",
  "author": "maxkrieger",
  "license": "MIT",
  "scripts": {
    "build:parsers": "nearleyc src/parser/Domain.ne > src/parser/DomainParser.ts && nearleyc src/parser/Substance.ne > src/parser/SubstanceParser.ts && nearleyc src/parser/Style.ne > src/parser/StyleParser.ts",
    "prebuild": "npm run build:parsers",
    "prestart": "npm run build:parsers",
    "test": "jest --watchAll=false",
    "test:watch": "jest",
    "build": "rollup -c",
    "start": "./build.js -watch",
    "pretest": "npm run build:parsers",
    "prebuild-lib": "npm run build:parsers",
    "docs": "typedoc --out docs src",
    "coverage": "npm test -- --coverage --watchAll=false",
    "lint": "eslint --ext js,ts,tsx src",
    "install-cli": "cd ../roger && npm install && npm link"
  },
  "dependencies": {
    "consola": "^2.15.2",
    "eigen": "^0.2.0",
    "fast-memoize": "^2.5.2",
    "graphlib": "^2.1.8",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "mathjax-full": "^3.0.1",
    "moo": "^0.5.1",
    "nearley": "^2.20.1",
    "seedrandom": "^3.0.5",
    "true-myth": "^4.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@types/graphlib": "^2.1.7",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.149",
    "@types/moo": "^0.5.3",
    "@types/nearley": "^2.11.1",
    "@types/node": "^10.17.21",
    "@types/seedrandom": "^2.4.28",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-jsdoc": "^30.7.3",
    "jest": "^26.6.3",
    "rollup": "^2.38.5",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.1.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.3"
  }
}
